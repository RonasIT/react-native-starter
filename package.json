{
  "name": "@ronas-it/react-native-starter",
  "description": "React Native + Expo starter template: a demo app with configurations, project structure, testing setup, navigation, and some basic modules.",
  "author": "Ronas IT",
  "bugs": {
    "url": "https://github.com/RonasIT/react-native-starter/issues"
  },
  "homepage": "https://github.com/RonasIT/react-native-starter/tree/main",
  "keywords": [
    "react-native",
    "template",
    "expo",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RonasIT/react-native-starter.git"
  },
  "version": "1.0.9",
  "scripts": {
    "start": "cross-env APP_ENV=development expo start",
    "start:prod": "cross-env APP_ENV=production expo start",
    "prebuild": "npm run check:ts && npm run check:cycles",
    "build:dev": "npm run prebuild && eas build --no-wait --non-interactive -p all --profile=development",
    "build:prod": "npm run build:dev -- --profile=production",
    "publish:dev": "npm run prebuild && cross-env APP_ENV=development expo publish --release-channel development",
    "publish:prod": "npm run prebuild && cross-env APP_ENV=production expo publish --release-channel production",
    "submit:ios:dev": "cross-env APP_ENV=development eas submit --no-wait -p ios --profile=development",
    "submit:ios:prod": "cross-env APP_ENV=production eas submit --no-wait -p ios --profile=production",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,.tsx ./",
    "format": "npx prettier --write **/*.{ts,tsx} && npx prettier --write *.{ts,tsx} && npm run lint -- --fix",
    "check:ts": "npx tsc",
    "check:cycles": "dpdm --no-warning --no-tree --transform --progress false index.js --exclude \".*?/?models/.*\\.tsx?|node_modules\" --exit-code circular:1",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "prepare": "husky install",
    "prepack": "node scripts/prepack.js",
    "postpack": "node scripts/postpack.js",
    "postinstall": "npm run postinit",
    "postinit": "path-exists app.json && (cpy npm-shrinkwrap.json package-lock.json --no-overwrite --rename && rimraf npm-shrinkwrap.json && rimraf app.json) || echo Project is already initialized"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.8",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/hooks": "^2.8.1",
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^1.0.0",
    "axios-observable": "^1.5.0",
    "class-transformer": "^0.5.1",
    "expo": "^46.0.0",
    "expo-application": "~4.2.2",
    "expo-constants": "~13.2.4",
    "expo-device": "~4.3.0",
    "expo-linking": "~3.2.2",
    "expo-localization": "~13.1.0",
    "expo-notifications": "~0.16.1",
    "expo-permissions": "~13.2.0",
    "expo-secure-store": "~11.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-updates": "~0.14.6",
    "i18n-js": "^3.9.2",
    "immer": "^9.0.12",
    "jwt-check-expiry": "^1.0.10",
    "lodash": "^4.17.21",
    "luxon": "^1.28.0",
    "qs": "^6.10.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.31.3",
    "react-native": "0.69.6",
    "react-native-expo-image-cache": "^4.1.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-svg": "12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-ui-lib": "^6.17.0",
    "react-redux": "^8.0.4",
    "redux-observable": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "sentry-expo": "~5.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.1.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^1.27.1",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "dpdm": "^3.10.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "lint-staged": "^13.0.3",
    "path-exists-cli": "^2.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
