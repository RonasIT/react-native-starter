{
  "name": "@ronas-it/react-native-starter",
  "description": "React Native + Expo starter template: a demo app with configurations, project structure, testing setup, navigation, and some basic modules.",
  "author": "Ronas IT",
  "bugs": {
    "url": "https://github.com/RonasIT/react-native-starter/issues"
  },
  "homepage": "https://github.com/RonasIT/react-native-starter/tree/main",
  "keywords": [
    "react-native",
    "template",
    "expo",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RonasIT/react-native-starter.git"
  },
  "version": "1.0.9",
  "scripts": {
    "start": "expo start --dev-client",
    "start:prod": "cross-env APP_ENV=production expo start",
    "prebuild": "npm run check:ts && npm run check:cycles",
    "build:dev": "npm run prebuild && eas build --no-wait --non-interactive -p all --profile=development",
    "build:prod": "npm run build:dev -- --profile=production",
    "update:dev": "npm run prebuild && cross-env APP_ENV=development eas update --branch development",
    "update:prod": "npm run prebuild && cross-env APP_ENV=production eas update --branch production",
    "submit:ios:dev": "cross-env APP_ENV=development eas submit --no-wait -p ios --profile=development",
    "submit:ios:prod": "cross-env APP_ENV=production eas submit --no-wait -p ios --profile=production",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,.tsx ./",
    "format": "npx prettier --write **/*.{ts,tsx} && npx prettier --write *.{ts,tsx} && npm run lint -- --fix",
    "check:ts": "npx tsc",
    "check:cycles": "dpdm --no-warning --no-tree --transform --progress false index.js --exclude \".*?/?models/.*\\.tsx?|node_modules\" --exit-code circular:1",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "prepare": "husky install",
    "prepack": "node scripts/prepack.js",
    "postpack": "node scripts/postpack.js",
    "postinstall": "npm run postinit",
    "postinit": "path-exists app.json && (cpy npm-shrinkwrap.json package-lock.json --no-overwrite --rename && rimraf npm-shrinkwrap.json && rimraf app.json) || echo Project is already initialized",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/hooks": "^2.8.1",
    "@react-native-community/netinfo": "9.3.5",
    "@react-navigation/bottom-tabs": "^6.4.3",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/stack": "^6.3.7",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-native-auth0": "^2.14.0",
    "axios": "^1.2.0",
    "axios-observable": "^2.0.0",
    "class-transformer": "^0.5.1",
    "expo": "~47.0.12",
    "expo-application": "~5.0.1",
    "expo-constants": "~14.0.2",
    "expo-device": "~5.0.0",
    "expo-linking": "~3.2.3",
    "expo-localization": "~14.0.0",
    "expo-notifications": "~0.17.0",
    "expo-permissions": "~14.0.0",
    "expo-secure-store": "~12.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-updates": "~0.15.6",
    "i18n-js": "^4.2.0",
    "immer": "^9.0.12",
    "jwt-check-expiry": "^1.0.10",
    "lodash": "^4.17.21",
    "luxon": "^1.28.0",
    "qs": "^6.11.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.3",
    "react-native": "0.70.5",
    "react-native-auth0": "^2.16.0",
    "react-native-expo-image-cache": "^4.1.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-ui-lib": "^6.21.2",
    "react-redux": "^8.0.5",
    "redux-observable": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "sentry-expo": "~5.0.0",
    "yup": "^0.32.11",
    "expo-clipboard": "~4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@testing-library/jest-native": "^5.1.2",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^1.27.1",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "dpdm": "^3.10.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-expo": "^47.0.0",
    "lint-staged": "^13.0.3",
    "path-exists-cli": "^2.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "~18.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "type-fest": "^3.0.0",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
